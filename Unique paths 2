class Solution:
    def uniquePathsWithObstacles(self, obstacleGrid: list[list[int]]) -> int:
        if not obstacleGrid or obstacleGrid[0][0] == 1:
            return 0
            
        m, n = len(obstacleGrid), len(obstacleGrid[0])
        
        # dp array to store number of ways to reach each column in the current row
        dp = [0] * n
        
        # Base case: there is 1 way to reach the starting point
        dp[0] = 1
        
        for r in range(m):
            for c in range(n):
                if obstacleGrid[r][c] == 1:
                    # If there's an obstacle, 0 ways to reach/pass through this cell
                    dp[c] = 0
                elif c > 0:
                    # Current ways = ways from above (dp[c]) + ways from left (dp[c-1])
                    dp[c] += dp[c-1]
                    
        return dp[-1]
