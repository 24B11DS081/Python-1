import math
import os
import random
import re
import sys
from collections import Counter

if __name__ == '__main__':
    s = input()
    
    # 1. Count character occurrences
    # Counter('aabbbccde') -> {'a': 2, 'b': 3, 'c': 2, 'd': 1, 'e': 1}
    chars = Counter(s).items()
    
    # 2. Sort: first by count (descending), then by character (ascending)
    # Using -x[1] flips the count for descending order while keeping x[0] ascending
    sorted_chars = sorted(chars, key=lambda x: (-x[1], x[0]))
    
    # 3. Print the top 3
    for char, count in sorted_chars[:3]:
        print(f"{char} {count}")
